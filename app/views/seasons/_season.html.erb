<div id="<%= dom_id season %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= season.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Start year:</strong>
    <%= season.start_year %>
  </div>
  <div>
    <strong class="block font-medium mb-1">End year:</strong>
    <%= season.end_year %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Studio:</strong>
    <%= season.studio_id %>
  </div>
</div>
